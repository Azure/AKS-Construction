          "authorizedIPRanges": {
            "value": [
              "20.36.64.4/32",
              "20.36.64.5/32"
            ]
          },
          "ingressApplicationGateway": {
            "value": true
          },
          "appGWcount": {
            "value": 2
          },
          "appGWsku": {
            "value": "WAF_v2"
          },
          "appGWmaxCount": {
            "value": 10
          },
          "appgwKVIntegration": {
            "value": true
          },
          "azureKeyvaultSecretsProvider": {
            "value": true
          },
          "createKV": {
            "value": true
          },
          "upgradeChannel": {
            "value": "stable"
          },
          "AksDiagCategories": {
            "value": [
              "cluster-autoscaler",
              "kube-apiserver",
              "kube-controller-manager",
              "kube-audit-admin",
              "kube-scheduler",
              "guard"
            ]
          },
          "acrUntaggedRetentionPolicyEnabled": {
            "value": true
          },
          "aksDisableLocalAccounts": {
            "value": true
          }
    }
