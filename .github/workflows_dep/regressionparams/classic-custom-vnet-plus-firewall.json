{
	"$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"resourceName": {
			"value": "customvnet"
		},
		"agentCount": {
			"value": 1
		},
		"agentVMSize": {
			"value": "Standard_DS3_v2"
		},
		"agentCountMax": {
			"value": 3
		},
		"custom_vnet": {
			"value": true
		},
		"bastion": {
			"value": true
		},
		"enable_aad": {
			"value": true
		},
		"enableAzureRBAC": {
			"value": true
		},
		"azureFirewalls": {
			"value": true
		},
		"omsagent": {
			"value": true
		},
		"retentionInDays": {
			"value": 30
		},
		"networkPolicy": {
			"value": "azure"
		},
		"azurepolicy": {
			"value": "audit"
		},
		"networkPlugin": {
			"value": "azure"
		},
        "KeyVaultSoftDelete" : {
            "value" : false
        },
        "KeyVaultPurgeProtection" : {
            "value" : false
        }
