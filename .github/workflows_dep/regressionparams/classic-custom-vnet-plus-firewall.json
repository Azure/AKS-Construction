{
	"$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"resourceName": {
			"value": "customvnet"
		},
		"kubernetesVersion": {
			"value": "1.20.9"
		},
		"agentCount": {
			"value": 1
		},
		"agentVMSize": {
			"value": "Standard_DS3_v2"
		},
		"agentCountMax": {
			"value": 3
		},
		"custom_vnet": {
			"value": true
		},
		"serviceCidr": {
			"value": "172.10.0.0/16"
		},
		"dnsServiceIP": {
			"value": "172.10.0.10"
		},
		"vnetAddressPrefix": {
			"value": "10.0.0.0/8"
		},
		"vnetAksSubnetAddressPrefix": {
			"value": "10.240.0.0/16"
		},
		"enable_aad": {
			"value": true
		},
		"enableAzureRBAC": {
			"value": true
		},
		"azureFirewalls": {
			"value": true
		},
		"vnetFirewallSubnetAddressPrefix": {
			"value": "10.241.0.0/24"
		},
		"omsagent": {
			"value": true
		},
		"retentionInDays": {
			"value": 30
		},
		"networkPolicy": {
			"value": "calico"
		},
		"azurepolicy": {
			"value": "audit"
		},
		"networkPlugin": {
			"value": "azure"
		},
        "KeyVaultSoftDelete" : {
            "value" : false
        },
        "KeyVaultPurgeProtection" : {
            "value" : false
        }
	}
}