{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceName": {
      "value": "cniov2"
    },
    "custom_vnet": {
      "value": true
    },
    "enable_aad": {
      "value": true
    },
    "aksDisableLocalAccounts": {
      "value": true
    },
    "enableAzureRBAC": {
      "value": true
    },
    "registries_sku": {
      "value": "Premium"
    },
    "omsagent": {
      "value": true
    },
    "retentionInDays": {
      "value": 30
    },
    "networkPolicy": {
      "value": "azure"
    },
    "networkPluginMode": {
      "value": "Overlay"
    },
    "vnetAksSubnetAddressPrefix": {
      "value": "10.240.0.0/28"
    }
  }
}
