{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "metadata": {
        "template": "bicep/main.bicep"
    },
    "parameters": {
        "location" : {
            "value": "westeurope"
        },
        "resourceName": {
            "value": "byo"
        },
        "agentCount": {
            "value": 1
        },
        "agentVMSize": {
            "value": "Standard_DS3_v2"
        },
        "agentCountMax": {
            "value": 2
        },
        "enable_aad": {
            "value": true
        },
        "enableAzureRBAC" : {
            "value": true
        },
        "registries_sku": {
            "value": "Premium"
        },
        "omsagent": {
            "value": true
        },
        "retentionInDays": {
            "value": 30
        },
        "networkPolicy": {
            "value": "azure"
        },
        "azurepolicy": {
            "value": "audit"
        },
        "availabilityZones": {
            "value": ["1","2","3"]
        },
        "ingressApplicationGateway": {
            "value": true
        },
        "byoAKSSubnetId": {
            "value": "/this/parameter/is/overridden/in/my/script/value/from/secrets"
        },
        "byoAGWSubnetId": {
            "value": "/this/parameter/is/overridden/in/my/script/value/from/secrets"
        },
        "serviceCidr" : {
            "value" : "172.10.0.0/16"
        },
        "dnsServiceIP" : {
            "value" : "172.10.0.10"
        },
        "appGWsku" : {
            "value" : "Standard_v2"
        },
        "SystemPoolType" : {
            "value" : "Standard"
        },
        "createKV" : {
            "value" : true
        },
        "azureKeyvaultSecretsProvider" : {
            "value" : true
        },
        "appgwKVIntegration" : {
            "value" : true
        },
        "upgradeChannel" : {
            "value" : "none"
        },
        "AksDiagCategories": {
            "value": [
              "cluster-autoscaler",
              "kube-apiserver",
              "kube-controller-manager",
              "kube-audit-admin",
              "kube-scheduler",
              "guard"
            ]
        },
        "KeyVaultSoftDelete" : {
            "value" : false
        },
        "KeyVaultPurgeProtection" : {
            "value" : false
        },
        "dnsZoneId" : {
            "value" : "/this/parameter/is/overridden/in/my/script/valuefromsecrets"
        }
    }
}
