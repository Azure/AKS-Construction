{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "metadata": {
      "template": "bicep/main.bicep"
    },
    "parameters": {
        "resourceName": {
            "value": "priv"
          },
          "agentVMSize": {
            "value": "Standard_DS3_v2"
          },
          "agentCountMax": {
              "value": 2
          },
          "JustUseSystemPool": {
            "value": true
          },
          "custom_vnet": {
            "value": true
          },
          "enable_aad": {
            "value": true
          },
          "aksDisableLocalAccounts": {
            "value": false
          },
          "enableAzureRBAC": {
            "value": true
          },
          "registries_sku": {
            "value": "Premium"
          },
          "acrPrivatePool": {
            "value": false
          },
          "azureFirewalls": {
            "value": true
          },
          "azureFirewallSku": {
            "value": "Basic"
          },
          "privateLinks": {
            "value": true
          },
          "omsagent": {
            "value": true
          },
          "retentionInDays": {
            "value": 30
          },
          "networkPolicy": {
            "value": "azure"
          },
          "azurepolicy": {
            "value": "deny"
          },
          "enablePrivateCluster": {
            "value": true
          },
          "ingressApplicationGateway": {
            "value": true
          },
          "bastion" : {
            "value": true
          },
          "appGWcount": {
            "value": 0
          },
          "appGWsku": {
            "value": "WAF_v2"
          },
          "appGWmaxCount": {
            "value": 2
          },
          "appgwKVIntegration": {
            "value": true
          },
          "keyVaultAksCSI": {
            "value": true
          },
          "keyVaultCreate": {
            "value": true
          },
          "upgradeChannel": {
            "value": "none"
          },
          "CreateNetworkSecurityGroups": {
            "value": true
          },
          "CreateNetworkSecurityGroupFlowLogs": {
            "value": true
          },
          "aksOutboundTrafficType" : {
              "value": "userAssignedNATGateway"
          },
          "createNatGateway" : {
              "value": true
          }
    }
}
