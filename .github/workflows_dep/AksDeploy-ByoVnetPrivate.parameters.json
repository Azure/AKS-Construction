{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "metadata": {
        "template": "bicep/compiled/main.json"
    },
    "parameters": {
        "resourceName": {
            "value": "priv"
          },
          "kubernetesVersion": {
            "value": "1.21.2"
          },
          "agentVMSize": {
            "value": "Standard_DS3_v2"
          },
          "agentCountMax": {
            "value": 3
          },
          "custom_vnet": {
            "value": true
          },
          "enable_aad": {
            "value": true
          },
          "aksDisableLocalAccounts": {
            "value": true
